create table type (
id serial primary key,
type_name varchar(2000)
);

create table product (
id serial primary key,
prod_name varchar(2000),
type_id int references type(id),
expired_date int,
price int,
quantity int
);

insert into type(type_name) values('СЫР');
insert into type(type_name) values('МОЛОКО');
insert into type(type_name) values('МОРОЖЕННОЕ');

insert into product(prod_name, type_id, expired_date, price, quantity) values('Моцарелла', 1, 12, 80, 5);
insert into product(prod_name, type_id, expired_date, price, quantity) values('Пармезан', 1, 1, 150, 20);
insert into product(prod_name, type_id, expired_date, price, quantity) values('Пломбир мороженное', 3, 12, 20, 8);
insert into product(prod_name, type_id, expired_date, price, quantity) values('Эскимо мороженное', 3, 5, 30, 12);
insert into product(prod_name, type_id, expired_date, price, quantity) values('Молоко 1%', 2, 12, 40, 5);
insert into product(prod_name, type_id, expired_date, price, quantity) values('Молоко 2%', 2, 1, 50, 12);

